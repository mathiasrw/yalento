{
  "name": "yalento",
  "version": "0.0.7",
  "description": "An awesome integration of Google Firebase for Angular and Node",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "test": "npm run build && nyc --reporter=lcov --reporter=text-summary node_modules/.bin/mocha --opts ./mocha.opts src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/"
  },
  "keywords": [
    "firebase"
  ],
  "author": "C-J",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/**/issues"
  },
  "homepage": "https://github.com/**r#readme",
  "dependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/validator": "^10.11.3",
    "alasql": "^0.5.1",
    "firebase": "^7.2.0",
    "guid-typescript": "^1.0.9",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.2.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.14.21",
    "assert": "^1.4.1",
    "chai": "^4.2.0",
    "jest": "^24.9.0",
    "mocha": "^6.2.1",
    "mochawesome": "^4.1.0",
    "nyc": "^15.0.0-beta.0",
    "prettier": "^1.18.2",
    "source-map-support": "^0.5.13",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.3"
  },
  "files": [
    "lib/**/*"
  ]
}
