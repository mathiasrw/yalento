{
  "name": "yalento",
  "version": "0.0.62",
  "description": "An awesome integration of Google Firebase for Angular and Node",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "test": "npm run build && nyc --reporter=lcov --reporter=text-summary node_modules/.bin/mocha --opts ./mocha.opts src",
    "test:ng8": "npm run build && cp -R lib test/ng8/node_modules/yalento/lib && cd test/ng8 && npm run test-headless"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/"
  },
  "keywords": [
    "firebase",
    "angular",
    "node.js",
    "alasql",
    "sequelizer",
    "typeorm",
    "state"
  ],
  "author": "C-J",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/miegli/yalento/issues"
  },
  "homepage": "https://github.com/miegli/yalento/blob/master/README.md",
  "dependencies": {
    "@firebase/firestore-types": "^1.6.2",
    "@types/bluebird": "^3.5.27",
    "@types/validator": "^10.11.3",
    "alasql": "^0.5.1",
    "class-transformer": "^0.2.3",
    "core-js": "^3.6.0",
    "es6-shim": "^0.35.5",
    "firebase": "^7.6.1",
    "guid-typescript": "^1.0.9",
    "js-sql-parser": "^1.2.1",
    "ngeohash": "^0.6.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3"
  },
  "devDependencies": {
    "@firebase/testing": "^0.5.3",
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.2.3",
    "@types/core-js": "^2.5.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.14.21",
    "assert": "^1.4.1",
    "chai": "^4.2.0",
    "chalk": "^2.4.1",
    "colors": "^1.3.2",
    "cross-env": "^5.2.0",
    "dotenv": "^8.2.0",
    "fs-extra": "^7.0.1",
    "inquirer": "^6.2.0",
    "lodash.camelcase": "^4.3.0",
    "log-update": "^2.3.0",
    "mocha": "^6.2.1",
    "mochawesome": "^4.1.0",
    "nyc": "^15.0.0-beta.0",
    "pegjs": "0.11.0-master.30f3260",
    "prettier": "^1.18.2",
    "prompt": "^1.0.0",
    "prompt-input": "^3.0.0",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.13",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.6.3"
  },
  "files": [
    "lib/**/*"
  ],
  "moduleDirectories": [
    "node_modules",
    "src"
  ]
}
